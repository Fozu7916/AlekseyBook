{
  "ConnectionStrings": {
    "DefaultConnection": "#{DATABASE_URL}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "#{JWT_SECRET}",
    "Issuer": "#{API_URL}",
    "Audience": "#{CLIENT_URL}"
  }
}
